{
  "publisher": "simonhe",
  "name": "unocss-to-css",
  "displayName": "unocss-to-css",
  "version": "0.0.10",
  "packageManager": "pnpm@8.1.1",
  "description": "Vscode extension transform unocss to css",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/unocss-to-css#readme",
  "repository": "https://github.com/Simon-He95/unocss-to-css",
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/unocss-to-css/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.77.0"
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:svelte",
    "onLanguage:jsx",
    "onLanguage:tsx",
    "onLanguage:astro",
    "onLanguage:vue",
    "onLanguage:swan",
    "onLanguage:wxml",
    "onLanguage:axml"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "npm compile && node ./node_modules/vscode/bin/test",
    "build": "tsup src/index.ts --external vscode",
    "package": "vsce package",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache --fix",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && npm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@types/node": "^18.15.11",
    "@types/vscode": "^1.77.0",
    "@unocss/core": "^0.50.6",
    "@unocss/preset-uno": "^0.50.6",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "magic-string": "^0.30.0",
    "pnpm": "^8.1.1",
    "tsup": "^6.7.0",
    "typescript": "^4.8.4",
    "unconfig": "^0.3.7",
    "vitest": "^0.29.8",
    "vsce": "^2.15.0"
  }
}
